{
  "engines": {
    "node": ">=2.0.0"
  },
  "name": "sap-site-entry",
  "version": "1.6.15",
  "description": "SAP Portal Approuter for Fiori Launchpad",
  "main": "server.js",
  "scripts": {
    "test": "npm run mocha",
    "start": "node --harmony server.js",
    "lint": "eslint -c node_modules/node-style/.eslintrc -f jslint-xml . > jshint.xml || true",
    "mocha": "node --harmony node_modules/istanbul/lib/cli.js cover  node_modules/mocha/bin/_mocha test/lib/ --recursive --report cobertura",
    "prepareRelease": "grunt && clean-packages && npm prune --production"
  },
  "author": "Zohar Team",
  "license": "Apache-2.0",
  "dependencies": {
    "approuter": "2.2.0",
    "body-parser": "1.15.0",
    "cfenv": "1.0.3",
    "commander": "2.8.1",
    "express": "4.12.3",
    "morgan": "1.6.1",
    "request": "2.69.0",
    "jwt-decode": "2.0.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "grunt": "^1.0.1",
    "grunt-openui5": "^0.9.0",
    "filter-node-package": "1.1.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "mockrequire": "0.0.5",
    "sinon": "^1.17.4",
    "qunitjs": "^1.23.1",
    "eslint": "3.2.2",
    "node-style": "^2.0.0"
  },
  "files": [
    "lib",
    "public",
    "services",
    "package.json",
    "README.md",
    "server.js"
  ]
}