{
  "name": "sap-hdbext",
  "version": "4.1.3",
  "description": "HDB extension library and utility functions for using SAP HANA in node.js",
  "main": "index.js",
  "engines": {
    "node": "^0.12.7 || ^4.4.0 || ^6.0.0",
    "npm": "^2.11.x"
  },
  "scripts": {
    "lint": "eslint -c node_modules/node-style/.eslintrc -f stylish lib/ index.js",
    "test": "node node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha test -- --recursive --check-leaks",
    "prepareRelease": "clean-packages && npm prune --production"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [
    "HDB",
    "hana",
    "sap",
    "sql",
    "hdi"
  ],
  "files": [
    "lib",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "index.js"
  ],
  "dependencies": {
    "accept-language": "2.0.16",
    "async": "1.5.0",
    "debug": "2.2.0",
    "generic-pool": "2.2.0",
    "hdb": "0.9.0",
    "lodash": "4.13.1",
    "lru-cache": "4.0.0",
    "sap-e2e-trace": "^1.0.3",
    "verror": "1.6.0",
    "winston": "1.0.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "eslint": "3.2.2",
    "express": "4.14.0",
    "filter-node-package": "^1.1.0",
    "istanbul": "0.4.4",
    "mocha": "3.0.2",
    "node-style": "2.0.0",
    "proxyquire": "1.7.10",
    "sap-xssec": "^0.8.0",
    "sinon": "1.17.5",
    "supertest": "2.0.0"
  }
}