namespace sap.common;

annotation Semantics 
{
    telephone 
    {
        type : array of String(10) enum {HOME='HOME'; CELL='CELL'; WORK='WORK'; FAX='FAX'; PREF='PREF'; TEXT='TEXT'; VOICE='VOICE'; VIDEO='VIDEO'; PAGER='PAGER'; TEXT_PHONE='TEXT_PHONE';};
    };
    eMail 
    {
        type : array of String(10) enum {HOME='HOME'; WORK='WORK'; PREF='PREF'; OTHER='OTHER';};
        address : Boolean;
    };
    name 
    {
        fullName : Boolean;
        givenName : Boolean;
        additionalName : Boolean;
        familyName : Boolean;
        nickName : Boolean;
        suffix : Boolean;
        prefix : Boolean;
        jobTitle : Boolean;
    };
    address 
    {
        type : array of String(10) enum {HOME='HOME'; WORK='WORK'; PREF='PREF'; OTHER='OTHER';};
        city : Boolean;
        street : Boolean;
        country : Boolean;
        region : Boolean;
        subRegion : Boolean;
        zipCode : Boolean;
        postBox : Boolean;
        label : Boolean;
    };
    organization 
    {
        name : Boolean;
        unit : Boolean;
        role : Boolean;
    };
    contact 
    {
        type : String(20) enum {PERSON='PERSON'; ORGANIZATION='ORGANIZATION'; };
        note : Boolean;
        photo : Boolean;
        birthDate : Boolean;
    };
}
	